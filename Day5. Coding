package Day5Coding;

public class Main {

    public static void main(String[] args) {

        // untuk variable
        String nim = "D0224007";
        char kelas = 'C';
        /* untuk print
         * dan untuk printf
         */

        System.out.print("NAMA     :MUH RIFAI \n");
        System.out.printf("NIM     : %s \n", nim);
        System.out.printf("KELAS   : %c \n", kelas);
    }
}
